<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<!-- items= "{/PartnerFunctions}" -->
	<TableSelectDialog
		noDataText= "{i18n>NO_PARTNERS_FUNCTIONS_FOUND}"
		title= "{i18n>PARTNER_FUNCTION_SEARCH}"
		rememberSelections= "false"
		contentWidth= "50%"
		class= "sapUiPopupWithPadding"
		search= "_handlePFValueHelpSearch"
		liveChange= "_handlePFValueHelpSearch"
		confirm= "_handleValueHelpClose"
		cancel= "_handleValueHelpClose">
		<ColumnListItem>
			<cells>
				<ObjectIdentifier title="{Description}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="8em">
				<header>
					<Text text="{i18n>PARTNER_FUNCTION}" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>
