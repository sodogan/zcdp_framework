<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <ViewSettingsDialog id="PartnersFilters" confirm="onHandleConfirm" resetFilters="onResetFilter">
        <filterItems>
            <ViewSettingsFilterItem id="partnerFunction" text="{i18n>PARTNER_FUNCTION_TXT}" multiSelect="false">
                <items>
                    <ViewSettingsItem text="{Description}" key="{PartnerFunction}" />
                </items>
            </ViewSettingsFilterItem>
        </filterItems>
    </ViewSettingsDialog>
</core:FragmentDefinition>
